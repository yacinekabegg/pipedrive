# Configuration d'environnement pour le pipeline Pipedrive
# Copiez ce fichier vers .env et configurez vos valeurs

# Configuration Pipedrive
PIPEDRIVE_API_KEY=your_pipedrive_api_key_here

# Configuration BigQuery
BIGQUERY_PROJECT_ID=your_project_id_here
BIGQUERY_LOCATION=US
BIGQUERY_DATASET_NAME=pipedrive_data

# Configuration du pipeline
PIPELINE_NAME=pipedrive
LOG_LEVEL=INFO

# Configuration pour le chargement incr√©mental (optionnel)
DEFAULT_SINCE_DAYS=7

